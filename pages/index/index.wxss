.title{
  position: absolute;
  color: #fff;
  top: 68rpx;
  left: 32rpx;
  z-index: 10;
}
.bg{
  width: 100%;
  height: 100vh;
  position: fixed;
}
.bg image{
  width: 100%;
  height: 100vh;
}
.dance{
  position: absolute;
  top: 480rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.dance image{
  height: 256rpx;
  width: 256rpx;
}
.tab{
  height: 100rpx;
  width: 100%;
  background: #090B1C;
  position: absolute;
  bottom: 0;
  display: flex;
  font-weight: bold;
}
.bar{
  height: 100%;
  width: 33%;
  line-height: 100rpx;
  font-size: 30rpx;
  color: #fce98d;
  text-align: center;
   text-shadow: 0 0 10px rgba(0,0,0,.9), 0 0 10px rgba(0,0,0,.9), 0 0 10px rgba(0,0,0,.8);
}